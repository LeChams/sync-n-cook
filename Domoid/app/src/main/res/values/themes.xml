    <color name="yellow">#ffff00</color>
    <color name="white">#ffffff</color>
    <color name="red">#ff0000</color>
    <color name="blue">#32add6</color>
    <color name="green">#00ff00</color>

    <style name="ThemeKitchen" parent="@android:style/Theme.Holo">
        <item name="android:background">@color/red</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ThemeBathRoom" parent="@android:style/Theme.Holo">
        <item name="android:background">@color/blue</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ThemeSalon" parent="@android:style/Theme.Holo">
        <item name="android:background">@color/green</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ThemeRoom" parent="@android:style/Theme.Holo">
        <item name="android:background">@color/yellow</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>
